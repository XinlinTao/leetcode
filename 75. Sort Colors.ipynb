    "def sortColors(nums):\n",
    "    count=[0]*3;\n",
    "    for i,c in enumerate(nums):\n",
    "        count[c]+=1;\n",
    "    index=0;\n",
    "    for i,c in enumerate(count):\n",
    "        for j in range(c):\n",
    "            nums[index]=i;\n",
    "            index+=1;\n",
    "    return(nums);"
   ]
