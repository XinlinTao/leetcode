    "def climbStairs(n: int) -> int:\n",
    "    if n==1:\n",
    "        return(1);\n",
    "    if n==2:\n",
    "        return(2);\n",
    "    if n>=3:\n",
    "        a=1;\n",
    "        b=2;\n",
    "        for i in range(2,n):\n",
    "            a,b=b,a+b;\n",
    "        return(b);"
   ]
