    "def maxProfit(prices):\n",
    "    if not prices:\n",
    "        return(0)\n",
    "    lowest=prices[0];\n",
    "    profit=0;\n",
    "    for i,p in enumerate(prices):\n",
    "        if p<lowest:\n",
    "            lowest=p;\n",
    "        if p-lowest>profit:\n",
    "            profit=p-lowest;\n",
    "    return(profit);"
   ]
  }
