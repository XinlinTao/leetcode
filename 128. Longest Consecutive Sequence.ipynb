    "def longestConsecutive( nums):\n",
    "    res=0\n",
    "    ss=set(nums)\n",
    "    for num in ss:\n",
    "        if num - 1 not in ss:\n",
    "            curr = num\n",
    "            ll = 1\n",
    "            while curr + 1 in ss:\n",
    "                curr += 1\n",
    "                ll += 1\n",
    "            res = max(res, ll)\n",
    "    return(res)"
   ]
  }
