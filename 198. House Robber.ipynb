    "class Solution:\n",
    "    def rob(self, nums: List[int]) -> int:\n",
    "        res1=0;\n",
    "        res2=0;\n",
    "        res=0\n",
    "        for num in nums:\n",
    "            curr=max(res1+num,res2);\n",
    "            res1=res2;\n",
    "            res2=curr;\n",
    "            res=max(res,res1,res2);\n",
    "        return(res)"
   ]
  }
